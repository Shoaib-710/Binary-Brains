public class Sum{
    public static void SumOddEven(int n){
        int so=0,se=0;
        while(n>0){
            if((n%10)%2==0)
                se += n%10;
            else
                so += n%10;
            n/=10;
        }
        System.out.println("sum of even integer = "+se);
        System.out.println("sum of odd integer = "+so);
    }
    public static void main(String args[]){
        SumOddEven(12345);
    }
}
